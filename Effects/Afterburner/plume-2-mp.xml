<?xml version="1.0"?>

<PropertyList>

 <path>plume.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>plume</object-name>
 </animation>

 <animation>
  <type>select</type>
  <object-name>plume</object-name>
  <condition>
   <equals>
     <property>sim/multiplay/generic/int[11]</property>
     <value>1</value>
   </equals>
  </condition>
 </animation>

 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5000</max-m>
 </animation>

</PropertyList>
