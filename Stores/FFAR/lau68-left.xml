<?xml version="1.0"?>

<!-- LAU-68 Rocket Launcher wrapper, by Dave Culp -->

<PropertyList>

 <path>LAU-68-left.ac</path>

 <animation>
  <type>select</type>
  <object-name>lau68</object-name>
  <condition>
   <not>
     <property>controls/armament/station1</property>
   </not>
  </condition>
 </animation>


